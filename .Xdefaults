*foreground:      #c0c0c0
*foreground_bold: #a8a19f
*cursor:          #a8a19f
*background:      #1b1918

URxvt.depth:            32
URxvt.fading:           10
URxvt.loginShell:       true
URxvt.saveLines:        1000
URxvt.internalBorder:   6
URxvt.lineSpace:        0
URxvt.letterSpace:      0

! Appearance
URxvt.font:             xft:DejaVuSansMono Nerd Font:pixelsize=13
URxvt.boldFont:         xft:DejaVuSansMono Nerd Font:bold:pixelsize=13
URxvt.italicFont:       xft:DejaVuSansMono Nerd Font:italic:pixelsize=13
URxvt.boldItalicFont:   xft:DejaVuSansMono Nerd Font:bold:italic:pixelsize=13

URxvt.background:       [90]#000000
URxvt.borderColor:      [90]#000000
URxvt.scrollBar:        false
URxvt.cursorBlink:      false
URxvt.cursorColor:      #af5f00
URxvt.cursorUnderline:  false

!URxvt.geometry:         166x56
!URxvt*backgroundPixmap: /home/michiel/dotfiles/Background/dwm-mypattern.png;style=tiled
URxvt*.transparent:     true
URxvt*.shading:         60
!URxvt*.blurRadius:      21
! Behavior
URxvt*scrollTtyOutput:   false
URxvt*scrollTtyKeypress: true

! Misc
! delimiters for double-click selection
URxvt*cutchars:          BACKSLASH '"'&()*,;<=>?@[]^{|}

! Xft settings
Xft.autohint:   0
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault

!! Extensions
URxvt.perl-ext-common: default,matcher,url-select,keyboard-select
URxvt.colorUL:         #4682B4
!! url-select
URxvt.keysym.M-u:      perl:url-select:select_next
URxvt.url-select.launcher: webview
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! Matcher
URxvt.url-launcher:   webview
URxvt.matcher.button: 1

rofi.color-enabled: true
rofi.color-window:  #000, #000, #000
rofi.color-normal:  #111, #819396, #222, #008ed4, #ffffff
rofi.color-active:  #002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:  #002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency: true
rofi.lines:             3
rofi.bw:                0
rofi.opacity:           "20"
rofi.hide-scrollbar:    true
rofi.width:             30

!! black dark/light
*color0: #494949
*color8: #5c5c5c

!! red dark/light
*color1: #870000
*color9: #cb4b16

!! green dark/light
*color2: #008700
*color10: #3eb53e

!! yellow dark/light
*color3: #ffaf5f
*color11: #fdd68a

!! blue dark/light
*color4: #5f87af
*color12: #9391bd

!! magenta dark/light
*color5: #d33682
*color13: #bd91b3

!! cyan dark/light
*color6: #009aa0
*color14: #8bb7b3

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3
