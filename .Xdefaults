*foreground:      #A0AEB0
*foreground_bold: #a8a19f
*cursor:          #a8a19f
*background:      #1b1918

URxvt.depth:            32
URxvt.fading:           0
URxvt.loginShell:       true
URxvt.saveLines:        1000
URxvt.internalBorder:   2
URxvt.lineSpace:        0
URxvt.letterSpace:      0

! Appearance
URxvt.font:             xft:DejaVuSansMono Nerd Font Mono:pixelsize=13
URxvt.boldFont:         xft:DejaVuSansMono Nerd Font Mono:bold:pixelsize=13
URxvt.italicFont:       xft:DejaVuSansMono Nerd Font Mono:italic:pixelsize=13
URxvt.boldItalicFont:   xft:DejaVuSansMono Nerd Font Mono:bold:italic:pixelsize=13

URxvt.intensityStyles:  false
URxvt.background:       [90]#000000
URxvt.scrollBar:        false
URxvt.cursorColor:      #d07500

URxvt.geometry:         166x56
URxvt*backgroundPixmap: /home/michiel/Pictures/Backgrounds/dwm.trans.png;style=tiled
URxvt*blendType:        darken

! Behavior
URxvt*scrollTtyOutput:   false
URxvt*scrollTtyKeypress: true

! Misc
! delimiters for double-click selection
URxvt*cutchars:          BACKSLASH '"'&()*,;<=>?@[]^{|}


! Xft settings
Xft.autohint:   0
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault

!! Extensions
URxvt.perl-ext-common: default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:         #4682B4
!! url-select
URxvt.keysym.M-u:      perl:url-select:select_next
URxvt.url-select.launcher: webview
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger:  C-Up
!! Matcher
URxvt.url-launcher:   webview
URxvt.matcher.button: 1

rofi.color-enabled: true
rofi.color-window:  #000, #000, #000
rofi.color-normal:  #111, #819396, #222, #008ed4, #ffffff
rofi.color-active:  #002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:  #002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency: true
rofi.lines:             3
rofi.bw:                0
rofi.opacity:           "20"
rofi.hide-scrollbar:    true
rofi.width:             30

